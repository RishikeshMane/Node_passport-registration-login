<% if(locals.errors){ %> <% errors.forEach(function(error) { %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
  <div class="card-body">         
          <p class="card-description"><%= error.msg %></p> 
  </div>     
<% }); %> <% } %> <% if(success_msg != ''){ %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
  <div class="card-body">       
          <p class="card-description"><%= success_msg %></p> 
  </div>     
<% } %> <% if(error_msg != ''){ %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
  <div class="card-body">
          <p class="card-description"><%= error_msg %></p> 
  </div>     
<% } %> <% if(error != ''){ %>
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
  <script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
  <div class="card-body">
          <p class="card-description"><%= error %>!</p> 
  </div>    
<% } %>














